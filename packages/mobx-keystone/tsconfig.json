{
  "extends": ["../../tsconfig.shared.all.jsonc"],
  "references": [
    { "path": "../lib/tsconfig.json" },
    { "path": "../ui/tsconfig.json" }
  ],
  "compilerOptions": {
    "lib": ["esnext", "dom", "dom.iterable"],
    "paths": {
      "#lib/*": ["../lib/src/*"],
      "#ui/*": ["../ui/src/*"],
      "#src/*": ["./src/*"]
    },
    "noEmit": true
  }
}
